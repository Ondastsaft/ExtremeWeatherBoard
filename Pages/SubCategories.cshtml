@page
@model ExtremeWeatherBoard.Pages.SubCategoriesModel
@{
    @section Sidebar {
        @if (Model.SideBarOptions != null)
        {
            <partial name="_SideBarPartial" model="Model.SideBarOptions" />
        }
    }
    @section MainContent {
        @if (Model.MainContent != null)
        {
            <partial name="_MainContentPartial" model="Model.MainContent" />
        }
    }
}
